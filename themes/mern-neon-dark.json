{
  "name": "Color Theme for Backend",
  "type": "dark",

  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "sideBar.background": "#181818",
    "activityBar.background": "#141414",
    "statusBar.background": "#141414",
    "editorLineNumber.foreground": "#555555"
  },

  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6a9955",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff6b6b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#f6c177",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "meta.object-literal.key"],
      "settings": {
        "foreground": "#4ade80",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#22d3ee",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": {
        "foreground": "#f472b6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#a78bfa",
        "fontStyle": "italic"
      }
    }
  ],

  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function": { "foreground": "#22d3ee", "italic": true },
    "variable": { "foreground": "#4ade80", "italic": true },
    "parameter": { "foreground": "#facc15", "italic": true },
    "property": { "foreground": "#4ade80", "italic": true }
  }
}
